#!/usr/bin/env python3

import sys

def factorize(number):
    for i in range(2, int(number ** 0.5) + 1):
        if number % i == 0:
            return i, number // i
    return number, 1

def main():
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        return

    file_path = sys.argv[1]
    
    with open(file_path, 'r') as f:
        lines = f.readlines()

    for line in lines:
        number = int(line.strip())
        p, q = factorize(number)
        print(f"{number}={p}*{q}")

if __name__ == "__main__":
    main()
