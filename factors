#!/usr/bin/env python3

import random
import math

def gcd(a, b):
    while b:
        a, b = b, a % b
    return abs(a)

def pollards_rho(n):
    x, c, y = random.randint(1, n-1), random.randint(1, n-1), random.randint(1, n-1)
    d = 1
    while d == 1:
        x = (x*x + c) % n
        y = (y*y + c) % n
        y = (y*y + c) % n
        d = gcd(x-y, n)
    return d

def factorize(n):
    d = n
    while d >= n:
        d = pollards_rho(n)
    return d, n // d

def main():
    if len(sys.argv) < 2:
        print("Usage: factors <file>")
        return

    file_path = sys.argv[1]
    numbers = read_numbers_from_file(file_path)

    for n in numbers:
        p, q = factorize(n)
        print(f"{n}={p}*{q}")

if __name__ == "__main__":
    main()
